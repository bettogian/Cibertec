<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
    <system.webServer>
        <directoryBrowse enabled="true" />
        <defaultDocument>
            <files>
                <add value="index.aspx" />
            </files>
        </defaultDocument>
      
    </system.webServer>

    <system.web>
        <globalization
            fileEncoding="utf-8" 
            requestEncoding="utf-8" 
            responseEncoding="utf-8"
            culture="sp-PE"
            uiCulture="sp-PE"
            />
    </system.web>
      
</configuration>